datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Article {
  id          Int      @id @default(autoincrement())
  title       String
  slug        String   @unique
  category    String
  summary     String
  content     String
  publishedAt DateTime
}
